{
  "name": "ember-cli-deploy-s3",
  "version": "2.0.0",
  "description": "An ember-cli-deploy plugin to upload to s3",
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "license": "MIT",
  "author": "Aaron Chambers and the ember-cli-deploy team",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ember-cli-deploy/ember-cli-deploy-s3",
  "scripts": {
    "release": "release-it",
    "test": "node tests/runner.js && ./node_modules/.bin/eslint index.js lib/* tests/**/*-test.js"
  },
  "dependencies": {
    "aws-sdk": "^2.792.0",
    "chalk": "^4.1.0",
    "core-object": "^3.1.5",
    "ember-cli-deploy-plugin": "^0.2.2",
    "lodash": "^4.17.20",
    "mime": "^2.4.6",
    "minimatch": "^3.0.3",
    "proxy-agent": "^4.0.0",
    "rsvp": "^4.8.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "^3.22.0",
    "ember-cli-terser": "^4.0.0",
    "eslint": "^7.11.0",
    "eslint-plugin-node": "^11.1.0",
    "github": "^0.2.4",
    "glob": "^7.1.1",
    "mocha": "^8.2.1",
    "multiline": "^2.0.0",
    "release-it": "^14.2.1"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "ember-addon": {
    "before": "ember-cli-deploy-redis"
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    }
  }
}
