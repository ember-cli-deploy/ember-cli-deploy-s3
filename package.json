{
  "name": "ember-cli-deploy-s3",
  "version": "3.3.0",
  "description": "An ember-cli-deploy plugin to upload to s3",
  "keywords": [
    "ember-addon",
    "ember-cli-deploy-plugin"
  ],
  "license": "MIT",
  "author": "Aaron Chambers and the ember-cli-deploy team",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/ember-cli-deploy/ember-cli-deploy-s3",
  "scripts": {
    "release": "release-it",
    "test": "node tests/runner.js && ./node_modules/.bin/eslint index.js lib/* tests/**/*-test.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1354.0",
    "chalk": "^4.1.0",
    "core-object": "^3.1.5",
    "ember-cli-deploy-plugin": "^0.2.2",
    "lodash": "^4.17.21",
    "mime": "^2.5.2",
    "minimatch": "^3.0.3",
    "proxy-agent": "^5.0.0",
    "rsvp": "^4.8.5"
  },
  "devDependencies": {
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "ember-cli": "^3.25.2",
    "eslint": "^7.21.0",
    "github": "^14.0.0",
    "glob": "^7.1.1",
    "mocha": "^8.3.1",
    "multiline": "^2.0.0",
    "release-it": "14.4.1",
    "release-it-lerna-changelog": "^3.1.0"
  },
  "engines": {
    "node": ">= 10.*"
  },
  "ember-addon": {
    "before": "ember-cli-deploy-redis"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": false
      }
    },
    "git": {
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": true
    }
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.17"
  }
}
